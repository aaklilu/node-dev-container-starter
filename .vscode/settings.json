{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "jest.pathToJest": "yarn jest",
  "files.associations": {
    ".env.**": "dotenv"
  },
  "search.exclude": {
    "**/lib/": true,
    "yarn-error.log": true,
    "yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.ignoreWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "cloudfunctions",
    "cloudsql",
    "corejs",
    "dataloader",
    "datname",
    "devtool",
    "downlevel",
    "envalid",
    "eslintcache",
    "esmodules",
    "esnext",
    "gamecenter",
    "globby",
    "gsutil",
    "jsonb",
    "knexfile",
    "kriasoft",
    "linkedin",
    "pathinfo",
    "sendgrid",
    "sourcemap",
    "tslib",
    "typechecking",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ]
}
